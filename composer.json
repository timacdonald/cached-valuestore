{
    "name": "axn/cached-valuestore",
    "description": "An extension of spatie/valuestore with in-memory caching.",
    "keywords": [
        "valuestore",
        "cache"
    ],
    "type": "utility",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim MacDonald",
            "email": "hello@timacdonald.me",
            "homepage": "https://timacdonald.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "TiMacDonald\\CachedValuestore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "spatie/valuestore": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
